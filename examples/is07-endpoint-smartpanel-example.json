    {
        "id": "filter_tally",
        "type": "switch",
        "z": "example_flow_3",
        "name": "Filter Tally Only",
        "property": "smartpanel.commands[0].type",
        "propertyType": "msg",
        "rules": [
            {
                "t": "eq",
                "v": "tally",
                "vt": "str"
            }
        ],
        "checkall": "false",
        "repair": false,
        "outputs": 1,
