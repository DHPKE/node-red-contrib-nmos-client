    {
        "id": "apply_tally",
        "type": "function",
        "z": "example_flow_3",
        "name": "Apply Tally State",
        "func": "// Extract tally command\nconst cmd = msg.smartpanel.commands[0];\n\n// Store in flow context\nconst currentState = flow.get('tally_state') || {};\ncurrentState[cmd.color] = cmd.state;\nflow.set('tally_state', currentState);\n\n// Prepare feedback message\nmsg.payload = {\n    action: 'send_status',\n    path: `tally/${cmd.color}/confirmed`,\n    value: cmd.state\n};\n\n// Add visual indicator data\nmsg.tally = {\n    color: cmd.color,\n    state: cmd.state,\n    displayText: cmd.state ? `${cmd.color.toUpperCase()} ON` : `${cmd.color.toUpperCase()} OFF`\n};\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 730,
        "y": 100
    },
    {
        "id": "send_feedback",
        "type": "link out",
        "z": "example_flow_3",
        "name": "Send Feedback",
        "links": ["link_feedback_in"],
        "x": 925,
        "y": 100
    },
    {
        "id": "link_feedback_in",
        "type": "link in",
        "z": "example_flow_3",
        "name": "Receive Feedback",
        "links": ["send_feedback"],
        "x": 175,
        "y": 180
    },
