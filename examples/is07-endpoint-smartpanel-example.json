        "disabled": false,
        "info": "# RIEDEL Smartpanel Command Processing\n\nThis flow demonstrates parsing Smartpanel commands.\n\n## Features:\n- Button press handling\n- Tally control\n- Fader/level processing\n- GPIO state monitoring"
    },
    {
        "id": "endpoint_2",
        "type": "nmos-is07-endpoint",
        "z": "example_flow_2",
        "name": "Smartpanel Receiver",
        "registry": "",
        "mqttBroker": "mqtt://localhost:1883",
        "deviceLabel": "Smartpanel Control Endpoint",
        "deviceDescription": "Receives commands from RIEDEL Smartpanel",
        "receiverLabel": "Control Receiver",
        "subscriptionFilter": "x-nmos/events/1.0/+/+",
        "mqttQos": 1,
        "sendStatusUpdates": true,
        "parseSmartpanel": true,
        "statusSourceId": "",
        "nodeId": "",
        "deviceId": "",
        "receiverId": "",
        "x": 340,
        "y": 100
    },
    {
        "id": "switch_command_type",
        "type": "switch",
        "z": "example_flow_2",
        "name": "Route by Command Type",
        "property": "smartpanel.commands[0].type",
        "propertyType": "msg",
        "rules": [
            {
                "t": "eq",
                "v": "button",
                "vt": "str"
            },
            {
                "t": "eq",
                "v": "tally",
                "vt": "str"
            },
            {
                "t": "eq",
                "v": "fader",
                "vt": "str"
            },
            {
                "t": "eq",
                "v": "gpio",
                "vt": "str"
            }
        ],
        "checkall": "false",
        "repair": false,
        "outputs": 4,
        "x": 590,
        "y": 100
    },
    {
        "id": "process_button",
        "type": "function",
        "z": "example_flow_2",
        "name": "Process Button",
        "func": "if (msg.smartpanel && msg.smartpanel.commands) {\n    const cmd = msg.smartpanel.commands[0];\n    \n    if (cmd.type === 'button' && cmd.pressed) {\n        msg.payload = {\n            action: 'button_press',\n            button: cmd.button,\n            timestamp: cmd.timestamp\n        };\n        \n        // Example: Map buttons to actions\n        switch(cmd.button) {\n            case 1:\n                msg.payload.action_name = 'Start Recording';\n                break;\n            case 2:\n                msg.payload.action_name = 'Stop Recording';\n                break;\n            case 3:\n                msg.payload.action_name = 'Take Snapshot';\n                break;\n            default:\n                msg.payload.action_name = `Button ${cmd.button}`;\n        }\n        \n        return msg;\n    }\n}\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "x": 840,
        "y": 40
    },
    {
        "id": "process_tally",
        "type": "function",
        "z": "example_flow_2",
        "name": "Process Tally",
        "func": "if (msg.smartpanel && msg.smartpanel.commands) {\n    const cmd = msg.smartpanel.commands[0];\n    \n    if (cmd.type === 'tally') {\n        msg.payload = {\n            action: 'tally_change',\n            color: cmd.color,\n            state: cmd.state,\n            timestamp: cmd.timestamp\n        };\n        \n        // Example: Control camera tally\n        if (cmd.color === 'red') {\n            msg.payload.meaning = cmd.state ? 'On Air (Program)' : 'Off Air';\n        } else if (cmd.color === 'green') {\n            msg.payload.meaning = cmd.state ? 'Preview' : 'Not in Preview';\n        }\n        \n        return msg;\n    }\n}\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "x": 840,
        "y": 80
    },
    {
        "id": "process_fader",
        "type": "function",
        "z": "example_flow_2",
        "name": "Process Fader",
        "func": "if (msg.smartpanel && msg.smartpanel.commands) {\n    const cmd = msg.smartpanel.commands[0];\n    \n    if (cmd.type === 'fader') {\n        // Convert normalized value (0.0-1.0) to dB (-60 to +12)\n        const dbLevel = (cmd.value * 72) - 60;\n        \n        msg.payload = {\n            action: 'fader_change',\n            fader: cmd.fader,\n            value_normalized: cmd.value,\n            value_db: Math.round(dbLevel * 10) / 10,\n            timestamp: cmd.timestamp\n        };\n        \n        return msg;\n    }\n}\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "x": 840,
        "y": 120
    },
    {
        "id": "process_gpio",
        "type": "function",
        "z": "example_flow_2",
        "name": "Process GPIO",
        "func": "if (msg.smartpanel && msg.smartpanel.commands) {\n    const cmd = msg.smartpanel.commands[0];\n    \n    if (cmd.type === 'gpio') {\n        msg.payload = {\n            action: 'gpio_change',\n            gpio: cmd.gpio,\n            state: cmd.state,\n            timestamp: cmd.timestamp\n        };\n        \n        return msg;\n    }\n}\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "x": 840,
        "y": 160
    },
