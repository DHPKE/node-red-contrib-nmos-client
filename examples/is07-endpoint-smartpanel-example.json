        "id": "join_cameras",
        "type": "join",
        "z": "example_flow_4",
        "name": "Collect Camera Events",
        "mode": "custom",
        "build": "array",
        "property": "payload",
        "propertyType": "msg",
        "key": "topic",
        "joiner": "\\n",
        "joinerType": "str",
        "accumulate": false,
        "timeout": "",
        "count": "3",
