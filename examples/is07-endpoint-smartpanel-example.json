    {
        "id": "production_logic",
        "type": "function",
        "z": "example_flow_4",
        "name": "Production Logic",
        "func": "// Centralized production logic\n// Process all camera tally events\n\nconst cameraStates = flow.get('camera_states') || {};\n\n// Update states from incoming events\nfor (const event of msg.payload) {\n    if (event.smartpanel && event.smartpanel.commands) {\n        const cmd = event.smartpanel.commands[0];\n        const deviceId = event.endpoint.device_id;\n        \n        if (!cameraStates[deviceId]) {\n            cameraStates[deviceId] = {};\n        }\n        \n        if (cmd.type === 'tally') {\n            cameraStates[deviceId][cmd.color] = cmd.state;\n        }\n    }\n}\n\nflow.set('camera_states', cameraStates);\n\n// Build production state summary\nmsg.payload = {\n    timestamp: new Date().toISOString(),\n    cameras: cameraStates,\n    program: findProgramCamera(cameraStates),\n    preview: findPreviewCamera(cameraStates)\n};\n\nfunction findProgramCamera(states) {\n    for (const [deviceId, state] of Object.entries(states)) {\n        if (state.red || state.program) {\n            return deviceId;\n        }\n    }\n    return null;\n}\n\nfunction findPreviewCamera(states) {\n    for (const [deviceId, state] of Object.entries(states)) {\n        if (state.green || state.preview) {\n            return deviceId;\n        }\n    }\n    return null;\n}\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 720,
        "y": 140
    },
